# Rofi 
super + p
		rofi -combi-modi window,drun -show combi -show-icons
# Pass manager
super + shift + p 
		rofi-pass -combi-modi window,drun -show combi -show-icons
# Terminal
super + t
		alacritty

super + shift + t
		urxvt
# Emacs
super + m 
		emacsclient -c -a 'emacs'
# Zathura
super + z
	zathura

# Polybar toggle
super + shift + b 
	--no-startup-id polybar-msg cmd toggle
# nemo
super + n
	nemo
# Pavucontrol
super + x 
	pavucontrol
# Aim lab
super + alt + a 
	steam steam://rungameid/714010

# Launch qutebrowser
super + q 
	qutebrowser
# Launch discord
super + shift + d
	discord

# Launch flameshot
super + shift + v
	flameshot

# Launch obsidian
super + shift + o
	flameshot

# Maim select screen
super + shift + s
	maim --select | xclip -selection clipboard -target image/png
super + alt + s
	maim --select ~/imgs/prints/print_01.png
# Screen recording
# ffmpeg -f x11grab -i :0.0 out.mkv
#Rotate screen
super + shift + n
	xrandr -o normal
super + shift + r
	xrandr -o right
#Select langauge

super + space + p
	setxkbmap br
super + space + u
	setxkbmap us

#Generate random id
super + shift + u 
	uuidgen | cut -c 1-4 | xclip -selection clipboard 

#Reload configs
super + shift + w
    pkill -usr1 -x sxhkd; notify-send 'sxhkd' 'Reloaded config'
