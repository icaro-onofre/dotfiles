# Dunst show last notification
super + shift + alt + n
	dunstctl history-pop
# Kitty
super + t
		kitty
# Rofi 
super + p
		rofi -combi-modi window,drun -show combi -show-icons

# Remmina
super + alt + r
	remmina

# Gnumeric
super + alt + g
	gnumeric

# Eclipse
super + alt + e
	eclipse

# Dbeave
super + alt + b
	dbeaver

# Notes 
super + shift + l 
		kitty nvim ~/baum/
# Emacs
super + m 
		emacsclient -c -a 'emacs'
# Zathura
super + z
	zathura

# Thunderbird
super + shift + t
	thunderbird

# Polybar toggle
super + shift + p 
	--no-startup-id polybar-msg cmd toggle
# nemo
super + n
	nemo
# Pavucontrol
super + x 
	pavucontrol
# Aim lab
super + alt + a 
	steam steam://rungameid/714010

# Launch qutebrowser
super + q 
	qutebrowser
# Launch discord
super + shift + d
	discord

# Launch flameshot
super + shift + v
	flameshot gui --clipboard

# Color picker
super + shift + b
	xcolor | xclip -sel clip	

# Launch spotify
super + alt + s
	spotify

# Launch obsidian
super + shift + o
	obsidian

# Launch obs-studio
super + alt + o
	obs

# Maim select screen
super + shift + s
	maim --select | xclip -selection clipboard -target image/png
super + ctrl + s
	xclip -sel clip -t image/png -o > ~/imgs/prints/img_$(uuidgen).png

# Screen recording
# ffmpeg -f x11grab -i :0.0 out.mkv

#Rotate screen
super + shift + n
	xrandr -o normal
super + shift + r
	xrandr -o right
#Select langauge

super + space + p
	setxkbmap br
super + space + u
	setxkbmap us

#Generate random id
super + shift + u 
	uuidgen | cut -c 1-4 | xclip -selection clipboard 

# Toggle polybar
super + alt + t
	/home/fforelle/.dotfiles/scripts/toggle-polybar.sh

#Reload configs
super + shift + w
    pkill -usr1 -x sxhkd; notify-send 'sxhkd' 'Reloaded config'
